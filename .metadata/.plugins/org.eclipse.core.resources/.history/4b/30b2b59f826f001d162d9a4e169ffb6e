import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		double A,B,C,D,R;
		
		A = sc.nextDouble();
		B = sc.nextDouble();
		C = sc.nextDouble();
		D = sc.nextDouble();

		R = (A * B - C * D );
		
		System.out.printf("Diferenca e: %.f%n", R);

		
		
		
}
}