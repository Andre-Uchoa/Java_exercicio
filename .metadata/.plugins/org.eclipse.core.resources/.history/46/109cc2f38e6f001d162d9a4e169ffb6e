import java.util.Scanner;

public class exercicio2 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		Double n,h,v,s;
		
		n = sc.nextDouble();
		h = sc.nextDouble();
		v = sc.nextDouble();
		
		s = h * v ;
		
		System.out.println("Number: " + n);
		System.out.printf("Salary: %.2f%n", s);
		

		
		
		
		
	sc.close();
	}
}
