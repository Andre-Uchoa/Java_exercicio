import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		double r, a, pi;
		
		pi = 3.14159;
		r = sc.nextDouble();
		a = pi * r * r;
		
		System.out.printf("A area do circulo foi: %.4f%n" , a);
		
		
		
		
		
		
		
		sc.close();
}
}